{
  "manifest_version": 3,
  "name": "Submit File Extension",
  "version": "1.0",
  "description": "Extension to submit files",
  "action": {
    "default_title": "Click to view a popup"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "debugger"
  ],
  "background": {
    "service_worker": "background.js"
  }
}